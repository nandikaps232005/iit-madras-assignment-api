{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "methods": ["POST"],
            "dest": "api/index.js"
        },
        {
            "src": "/api/(.*)",
            "methods": ["POST"],
            "dest": "api/index.js"
        }
    ]
}